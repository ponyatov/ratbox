PROJECT_NAME           = $(MODULE)
PROJECT_NUMBER         = ver: $(REL)-$(BRANCH) ($(NOW))
PROJECT_BRIEF          = "https://freelance.habr.com/tasks/592676"
HTML_OUTPUT            = docs
OUTPUT_DIRECTORY       =
INPUT                  = inc src tmp $(MODULE) README.md
INCLUDE_PATH           = inc src tmp $(MODULE)
EXCLUDE                = ref/*
WARN_IF_UNDOCUMENTED   = NO
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = README.md
GENERATE_LATEX         = NO
FILE_PATTERNS         += *.lex *.yacc *.ino
EXTENSION_MAPPING      = lex=C++ yacc=C++ ino=C++
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
LAYOUT_FILE            = doc/DoxygenLayout.xml
SORT_GROUP_NAMES       = YES
